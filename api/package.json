{
  "name": "@cerply/api",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsup src/index.ts --out-dir dist --format cjs --sourcemap",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "start:dev": "tsx watch src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "seed:demo": "node scripts/seed_demo.js",
    "test": "vitest run --reporter=verbose"
  },
  "dependencies": {
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/multipart": "7.7.3",
    "dotenv": "^17.2.1",
    "fastify": "^4.29.1",
    "mammoth": "^1.10.0",
    "openai": "^5.16.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "pino": "^9.3.2",
    "undici": "^7.13.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/pg": "^8.10.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "tsx": "^4.15.7",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  }
}
