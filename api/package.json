{
  "name": "@cerply/api",
  "main": "dist/index.js",
  "private": true,
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "clean": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "start:dev": "tsx watch src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "seed:demo": "node scripts/seed_demo.js",
    "test": "node scripts/test-runner.mjs",
    "test:adaptive": "vitest run tests/adaptive.test.ts",
    "test:quality-floor": "CANON_ENABLED=true vitest run tests/quality-floor.test.ts",
    "test:cost-graph": "CANON_ENABLED=true vitest run tests/cost-graph.test.ts tests/canon-reuse.test.ts",
    "test:epic7-migration": "vitest run tests/epic7-migration-compatibility.test.ts",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:seed": "tsx scripts/seed.ts",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init_admin_store",
    "openapi:build": "tsx scripts/openapi-build.ts",
    "openapi:check": "npm run openapi:build && test -f openapi/build/openapi.json",
    "planner:eval": "tsx scripts/planner-eval.ts",
    "planner:eval:adaptive": "tsx scripts/planner-eval.adaptive.ts",
    "planner:eval:openai": "tsx scripts/planner-eval.openai.ts",
    "planner:eval:multi": "tsx scripts/planner-eval.multiphase.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@fastify/cookie": "^9.4.0",
    "@fastify/cors": "^8.4.2",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "7.7.3",
    "@fastify/rate-limit": "^8.0.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^5.20.0",
    "@types/fast-levenshtein": "^0.0.4",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.6",
    "fast-levenshtein": "^3.0.0",
    "fastify": "^4.29.1",
    "ioredis": "^5.4.1",
    "mammoth": "^1.10.0",
    "openai": "^5.16.0",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "pino": "^9.3.2",
    "undici": "^7.13.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.15",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^1.6.1",
    "drizzle-kit": "^0.31.5",
    "esbuild": "^0.21.5",
    "magic-string": "^0.30.19",
    "picocolors": "^1.1.1",
    "prisma": "^5.20.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "tsup": "^8.0.2",
    "tsx": "^4.15.7",
    "typescript": "^5.4.5",
    "vite": "^5.4.20",
    "vitest": "^1.6.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.21.0"
  }
}
