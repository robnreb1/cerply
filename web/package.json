{
  "name": "@cerply/web",
  "description": "Fixed cache no-store issues",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3000",
    "dev:ent": "cross-env NEXT_PUBLIC_ENTERPRISE_MODE=true next dev -H 0.0.0.0 -p 3001",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "smoke:vercel": "bash ./scripts/vercel-smoke.sh",
    "smoke:api": "node ./scripts/smoke-api.mjs",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:e2e": "node ./lib/presenter/certifiedPlan.test.mjs && cross-env NEXT_PUBLIC_PREVIEW_CERTIFIED_UI=true npm run -s build && cross-env NEXT_PUBLIC_PREVIEW_CERTIFIED_UI=true playwright test e2e/certified.plan.spec.ts e2e/certified.study.resume.spec.ts --config=playwright.config.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "lucide-react": "^0.539.0",
    "next": "^14.2.31",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1",
    "@playwright/test": "^1.48.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.21",
    "cross-env": "^10.0.0",
    "jsdom": "^27.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "vitest": "^1.6.1"
  }
}
