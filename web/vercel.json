{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "x-vercel-json", "value": "loaded" }]
    }
  ],
  "rewrites": [],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "framework": "nextjs",

  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    },
    "pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "NEXT_TELEMETRY_DISABLED": "1"
    }
  },
  "env": {
    "NODE_VERSION": "20"
  }
}
